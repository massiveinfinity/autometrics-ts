{
  "private": true,
  "name": "autometrics-monorepo",
  "packageManager": "yarn@3.6.3",
  "version": "1.0.1-mi.3",
  "workspaces": [
    "dist/autometrics",
    "dist/exporter-otlp-http",
    "dist/exporter-prometheus",
    "dist/exporter-prometheus-push-gateway",
    "packages/parcel-transformer-autometrics",
    "packages/typescript-plugin"
  ],
  "scripts": {
    "build": "just build",
    "deploy": "yarn run deploy:autometrics && yarn run deploy:exporter-prometheus && yarn run deploy:push-gateway",
    "deploy:autometrics": "cd dist/autometrics && npm publish --access public",
    "deploy:exporter-prometheus": "cd dist/exporter-prometheus && npm publish --access public",
    "deploy:push-gateway": "cd dist/exporter-prometheus-push-gateway && npm publish --access public"
  },
  "devDependencies": {
    "@opentelemetry/api": "1.7.0",
    "@opentelemetry/core": "1.18.0",
    "@opentelemetry/exporter-metrics-otlp-http": "0.45.0",
    "@opentelemetry/exporter-prometheus": "0.45.0",
    "@opentelemetry/resources": "1.18.0",
    "@opentelemetry/sdk-metrics": "1.18.0",
    "@parcel/types": "^2.10.0",
    "@types/express": "^4.17.21",
    "@types/node": "^18.6.5",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@opentelemetry/sdk-metrics": "1.18.0"
  }
}
